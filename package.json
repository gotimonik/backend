{
  "name": "backend-sample",
  "version": "1.0.0",
  "description": "Backend Sample",
  "main": "src/app.ts",
  "scripts": {
    "build:env": "printenv > .env",
    "compile": "tsc",
    "clean": "rm -rf lib",
    "build": "npm run compile",
    "gh-build": "npm run clean && npm run custom-lint && npm run compile",
    "start": "node lib/app.js",
    "dev": "npm run build && node lib/app.js",
    "dev:watch": "nodemon",
    "test": "jest; jest-coverage-badges",
    "lint": "eslint */**/*.{js,ts,tsx,jsx} --fix",
    "custom-lint": "eslint '*/**/*.{js,ts,tsx,jsx}' --fix -f ./scripts/eslintCustomFormatter.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Miral26/backend"
  },
  "keywords": [
    "template",
    "app engine",
    "airbnb",
    "insight timer",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Insight Timer",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.4",
    "csv-parse": "^4.16.0",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.13",
    "method-override": "^3.0.0",
    "node-cache": "^5.1.2",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "qs": "^6.10.1",
    "shortid": "^2.2.16",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@semantic-release/github": "^7.0.7",
    "@types/cls-hooked": "^4.3.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.8",
    "@types/lodash": "^4.14.118",
    "@types/method-override": "0.0.31",
    "@types/node": "^10.14.8",
    "@types/shortid": "0.0.29",
    "@types/supertest": "^2.0.9",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "jest-coverage-badges": "^1.1.2",
    "nodemon": "^2.0.1",
    "semantic-release": "^17.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "bugs": {
    "url": "https://github.com/Miral26/backend/issues"
  },
